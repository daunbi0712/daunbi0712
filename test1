// 13번 문제
package eui;
import java.util.Scanner;
public class te006 {

	public static void main(String[] args) {
		Scanner inp = new Scanner(System.in);
		int x[] = new int[10];
		
		while(ture) {
			int a = inp.nextInt();
			if(a==0) {
				break;
			}
			else{
				a/=10;
				x[a-1]++;
			}
		}
		for(int j=x.length-1;j>=0;j--) {
			if(x[j]!=0) {
				System.out.printf("%d : %dperson\n", (j+1)*10,x[j]);
			}
		}
	}
}
